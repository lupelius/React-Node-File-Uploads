{
  "name": "docker-react",
  "version": "1.0.0",
  "description": "React based single page application that allows file uploads, views and search.",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.2.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "$(npm bin)/flow && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "flow": "$(npm bin)/flow"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": "Serdar Senay <serdar@tokentalk.co>",
  "license": "ISC",
  "devDependencies": {
    "flow-bin": "^0.104.0",
    "react-test-renderer": "^16.8.6"
  }
}
